t.home()

t.clear()



for j in range(2):

  for i in range(4):

    t.forward(100)

    t.left(90)

  

  t.left(90)

  t.forward(120)

  

  for i in range(6):

    t.forward(100)

    t.forward(-100)

    t.left(-90)

    t.forward(20)

    t.left(90)

  

  t.forward(-120)

  t.left(-90)

  t.forward(-240)



