---

def total(n):
  s=0
  for x in range(1,n+1):
    s+=x
   return s
   
total(100)

total(10)

total(1000)

---

def rect(x, y, a):

  t.up()

  t.home()

  t.forward(x)

  t.left(90)

  t.forward(y)

  t.down()

  for i in range(4):

    t.forward(a)

    t.left(-90)



def yline(x, y, a):

  t.up()

  t.home()

  t.forward(x)

  t.left(90)

  t.forward(y)

  t.down()

  t.forward(a)